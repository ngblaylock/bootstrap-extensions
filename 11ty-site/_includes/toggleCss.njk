<div id="cssToggle"></div>

<button class="btn" onclick="toggleCss()">Toggle CSS</button>
<script>
  let selectedCssValue = 0;
  function toggleCss() {
    const css = [
      `<link rel="stylesheet" href="/css/site.min.css" />`,
      `<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous" /><link rel="stylesheet" href="/css/bootstrap-extensions.min.css" />`
    ]

    selectedCssValue = selectedCssValue ? 0 : 1;
    
    document.getElementById('cssToggle').innerHTML = css[selectedCssValue];
  }
  toggleCss();
</script>
